    "karma-coverage": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-tap": "^4.1.4",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "puppeteer": "^2.0.0",
